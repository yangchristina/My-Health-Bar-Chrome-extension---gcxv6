{
    "manifest_version": 2,
    "name": "p5 sketch2",
    "version": "0.1",
    "permissions": ["background", "alarms", "idle"],
    "web_accessible_resources": [
      "assets/*.png"
    ],
    "content_scripts": [{
      "matches": [
        "<all_urls>"
      ],
      "js": ["p5.js", "sketch.js"]
    }],
    "background":{ 
      "scripts": ["p5.js", "background.js"],
      "persistent": false
    },
    "browser_action": {
      "default_icon": "assets/heart.png"
    }
}